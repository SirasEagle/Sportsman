{% extends 'base.html.twig' %}

{% block title %}Muscle Groups{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/musclegroup.css') }}">
{% endblock %}
{% block body %}

<div class="example-wrapper">
    <h2>Muscle Groups</h2>
    <a href="{{ path('new_muscle_group') }}" class="button">New Muscle Group</a>
    {% if musclegroups %}
        <h3>{{ "All" }}</h3>
        <div>
            {% for musclegroup in musclegroups %}
                <div>
                    <div>{{ musclegroup.term }}</div>
                </div>
            {% endfor %}
        </div>
    {% endif %}
</div>
{% endblock %}
