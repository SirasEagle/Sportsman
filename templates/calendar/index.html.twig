{% extends 'base.html.twig' %}

{% block title %}Hello CalendarController!{% endblock %}

{% block body %}

<div class="example-wrapper">
    {% if workouts %}
        {% for workout in workouts %}
            {{ workout.date|date('Y-m-d') }}
        {% endfor %}
    {% endif %}
    <a href="{{ path('new_workout') }}" class="button">New Workout</a>
</div>
{% endblock %}
